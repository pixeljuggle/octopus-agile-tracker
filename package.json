{
  "name": "vite-boilerplate",
  "private": true,
  "version": "0.0.0",
  "homepage": "https://pixeljuggle.github.io/octopus-agile-tracker/",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build --base=./",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "preview": "vite preview",
    "prepare": "husky install",
    "lint": "eslint --ext .js,.ts,.tsx ./src --ignore-path .gitignore",
    "clean": "gts clean",
    "compile": "tsc",
    "check-types": "tsc --project tsconfig.json --pretty --noEmit",
    "check": "gts check && npm run check-types",
    "fix": "gts fix",
    "pretest": "npm run compile",
    "posttest": "npm run lint",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@floating-ui/react": "^0.24.3",
    "@headlessui/react": "^1.7.15",
    "@radix-ui/react-dialog": "^1.0.4",
    "@radix-ui/react-dropdown-menu": "^2.0.5",
    "dayjs": "^1.11.8",
    "framer-motion": "^10.12.12",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.10.0",
    "recharts": "^2.7.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.5.1",
    "@commitlint/config-conventional": "^17.4.4",
    "@storybook/addon-essentials": "^7.0.18",
    "@storybook/addon-interactions": "^7.0.18",
    "@storybook/addon-links": "^7.0.18",
    "@storybook/addon-mdx-gfm": "^7.0.18",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/blocks": "^7.0.18",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^7.0.18",
    "@storybook/react-vite": "^7.0.18",
    "@storybook/testing-library": "^0.1.0",
    "@types/node": "^14.11.2",
    "@types/react": "^18.2.5",
    "@types/react-dom": "^18.0.11",
    "@types/react-table": "^7.7.14",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "@vite-pwa/assets-generator": "^0.0.3",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-absolute-imports": "^0.0.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-storybook": "^0.6.12",
    "gh-pages": "^5.0.0",
    "gts": "^3.1.1",
    "husky": "^8.0.3",
    "postcss": "^8.4.21",
    "prettier": "^2.8.7",
    "prettier-plugin-tailwindcss": "^0.2.7",
    "prop-types": "^15.8.1",
    "sass": "^1.62.0",
    "storybook": "^7.0.18",
    "tailwindcss": "^3.3.1",
    "typescript": "^4.9.3",
    "vite": "^4.2.0",
    "vite-plugin-pwa": "^0.16.4",
    "vite-tsconfig-paths": "^4.2.0",
    "workbox-window": "^7.0.0"
  }
}
